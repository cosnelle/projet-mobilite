
// boucle dans le fichier main
//  pour obtenir le nbr de pers par univ
x = []
for idx, name in enumerate(df['Etablissement'].value_counts().index.tolist()):
	  res = {
	    "Etablissement": name,
      "Nb_person" : int(df['Etablissement'].value_counts()[idx])
	  }
	  x.append(res)
	
  data = {
	  "data" : x
  }